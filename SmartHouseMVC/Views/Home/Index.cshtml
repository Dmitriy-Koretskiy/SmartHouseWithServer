@using SmartHouseWebSite.Models
@model IEnumerable<RoomViewModel>

@{
    ViewBag.Title = "Tables";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>
    @foreach (RoomViewModel room in Model)
    {   
         <p>@Html.RouteLink(room.Name, "Room", new { action = "Index", controller = "Room", roomId = room.Id })</p>    
    }
</div>

